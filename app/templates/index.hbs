<div id="bryPage">
  <div class="col-md-9" id="bryBikerows">
    <ul>
      {{#each model as |bike|}}
        <div id="bryBike">

          <div class="col-md-6" id="bryImage">
            <img src="{{bike.bikePhoto3}}" alt="{{bike.name}}" width='250px' height='200px'>
          </div>

          <div class="col-md-6" id="bryDetails">
            <h4>{{bike.name}}</h4>
            <br>
            <p>{{bike.description}}</p>
            <button type="button" class="btn btn-danger">More</button>
              <hr>
          </div>

        </div>

      {{/each}}
    </ul>
  </div>
  <div class="col-md-3" id="brySideBar">

  </div>

</div>
